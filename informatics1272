//HELLO WORLD by http://codeforces.com/profile/Programmer
#include <stdio.h>
#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
const ll INF=1e18;
typedef double d;
typedef long double lld;

ll my_pow (ll a, ll n) {
    int res = 1;
    while (n)
        if (n & 1) { res *= a; --n; }
        else { a *= a; n
            >>= 1; }
    return res; }
const int MAXN = 1e5 + 100;
int color[MAXN];
bool finish = false;
vector <int> g[MAXN], first, second;
void DFS(int v, int cl, int p)
{
    if (color[v] != 0)
        return;
    color[v] = cl;
    if (cl == 2)
    {
        cl = 1;
        second.push_back(v);
    }
    else
    {
        cl = 2;
        first.push_back(v);
    }
    for (auto it:g[v])
        if (it != p)
            DFS(it, cl, v);
}
void prov(int v)
{
    if (!color[v])
        return;
    int cl = color[v];
    color[v] = 0;
    for (auto it:g[v])
        if (color[it] == cl)
        {
            finish = true;
            return;
        }
    for (auto it:g[v])
        prov(it);
}
int main()
{
    freopen("input.txt", "r", stdin);
    int n, m, x, y;
    cin>>n>>m;
    for (int i = 0; i < m; i++)
    {
        cin>>x>>y;
        g[x].push_back(y);
        g[y].push_back(x);
    }
    for (int i = 1; i <= n; i++)
        if (!color[i])
            DFS(i, 1, 0);
    for (int i = 1; i <= n; i++)
        if (color[i])
            prov(i);
    if (finish)
    {
        cout<<0;
        return 0;
    }
    sort(first.begin(), first.end());
    sort(second.begin(), second.end());
    for (auto it:first)
        cout<<it<<" ";
    cout<<'\n';
    for (auto it:second)
        cout<<it<<" ";
return 0;
}
