//HELLO WORLD by http://codeforces.com/profile/Programmer
#include <stdio.h>
#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
const ll INF=1e18;
typedef double d;
typedef long double lld;

ll my_pow (ll a, ll n) {
    int res = 1;
    while (n)
        if (n & 1) { res *= a; --n; }
        else { a *= a; n >>= 1; }
    return res; }
const int MAXN = 1e5 + 100;
int cl[MAXN];
bool used[MAXN];
vector <int> g[MAXN], gr[MAXN], topsort;
pair <int, int> ribs;
void DFS_sort(int v)
{
    used[v] = true;
    for (auto it:g[v])
        if (!used[it])
            DFS_sort(it);
    topsort.push_back(v);
}
void DFS_condens(int v, int color)
{
    cl[v] = color;
    for (auto it:gr[v])
        if (!cl[it])
            DFS_condens(it, color);
}
int main()
{
    freopen("input.txt", "r", stdin);
    int n, m, h, x, y;
    cin>>n>>m>>h;
    for (int i = 0; i < m; i++)
    {
        cin>>x>>y;
        g[x].push_back(y);
        gr[y].push_back(x);
        ribs.push_back({x, y});
    }
    for (int i = 1; i <= n; i++)
        if (!used[i])
            DFS_sort(i);
    int color = 1;
    for (int i = (int)topsort.size()-1; i >= 0; i--)
        if (!cl[topsort[i]])
        {
            DFS_condens(topsort[i], color++);
        }
    vector <int> gc[color];
    for (auto it:ribs)
    {
        x = it.first;
        if (cl[x] != cl[y])
            rib[cl[x]].push_back(cl[y]);
    }
return 0;
}
